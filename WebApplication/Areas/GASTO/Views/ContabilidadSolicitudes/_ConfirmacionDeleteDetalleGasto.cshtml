

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
   
        <h3 class="modal-title" id="myModalLabel"><i class="fa fa-eur"></i><i class="fa fa-chevron-right"></i>CONFIRMACION ELIMINACION</h3>
   

</div>
<div class="modal-body">
    <input type="hidden" id="hdfIdDetRendicionSolicitudEliminar" value="@ViewBag.hdfIdDetRendicionSolicitudEliminar" />
    <br />


    <div id="nav-tab-content" class="tab-content">
        <div class="tab-pane active" id="info" role="tabpanel">
            <label class="col-sm-12">Está seguro de eliminar el Detalle de Gasto Seleccionado</label>

        </div>
    </div>
    <br />
    <br />
</div>
<div class="modal-footer">
    <div style="float: right">
        <button id="btnEliminarDetalleGasto" class="btn btn-primary btn-sm"><i class="fa fa-floppy-o"></i>Eliminar</button>
        <button class="btn btn-default btn-sm" data-dismiss="modal"><i class="fa fa-share-square-o"></i> Cancelar</button>
    </div>
</div>
</div> @*FIN MODAL*@

<script>
    $(document).ready(function () {
        $('#btnEliminarDetalleGasto').click(function (e) {
            e.preventDefault();
            document.getElementById("btnEliminarDetalleGasto").disabled = true;
            var id = $('#hdfIdDetRendicionSolicitudEliminar').val();
            EliminarDetalleGasto(id);
           
        });
    });
</script>
